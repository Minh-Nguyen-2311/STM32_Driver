#include "DAC_Process.h"

static uint32_t DAC_Init_(OOP const * const me);
static void DAC_Process_(OOP const * const me);

void DAC_buf_ctor(DAC_st * const me, void *data, uint8_t ID_Signal, uint8_t num_dac)
{
	static struct VirtualTable const vtbl = {
			/* vtbl of the UART class */
			&DAC_Init_,
			&DAC_Process_
	};

	OOP_ctor(&me->super, data); /* call superclass' ctor */

	me->super.vptr = &vtbl;  /* override the vptr */
	me->super.data = NULL;

	me->DAC_data = 0;

	me->ID_Signal = ID_Signal;
	me->num_dac = num_dac;
}

static uint32_t DAC_Init_(OOP const * const me)
{
	DAC_st * const me_ = (DAC_st *)me;
	(void)me_; //Avoid compiler error
	return 0;
}
